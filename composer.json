{
    "name": "logicbrush/metrics",
    "description": "Examines the clover metrics for the project and will update method Docblocks in the provided source file with `@Metric` annotations.",
    "keywords": ["metrics","annotations","clover"],
    "homepage": "https://github.com/logicbrush/metrics",
    "license": "Apache License 2.0",
    "authors": [
        {
            "name": "John Learn",
            "email": "jflearn@logicbrush.com"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "doctrine/annotations": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "Logicbrush\\Metrics\\": "src"
        }
    },
    "bin": [
        "bin/annotate"
    ],
    "prefer-stable": true
}
